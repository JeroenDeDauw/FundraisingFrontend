{
  "name": "wmde_fundraising_cat17",
  "version": "1.0.0",
  "description": "WMDE fundraising application (end user facing part)",
  "main": "index.js",
  "directories": {
    "lib": "src/app/lib",
    "test": "src/app/tests"
  },
  "scripts": {
    "watch": "gulp watch",
    "watch-faq": "watchify -vd -p browserify-hmr -e faq-page/src/main.js -o ../../web/skins/cat17/scripts/faq.js",
    "build-faq": "cross-env NODE_ENV=production browserify -g envify -p [ vueify/plugins/extract-css -o web/css/faq.css ] -e faq-page/src/main.js | uglifyjs -c warnings=false -m > web/scripts/faq.js",
    "lint": "eslint --ext .js,.vue faq-page",
    "test": "tape 'src/app/tests/**/*.js' | tap-min",
    "cs": "jshint src/app && jscs src/app",
    "ci": "npm run cs && npm run test && npm run build-assets",
    "build-assets": "npm-run-all --parallel webpack-build-assets build-css build-faq",
    "build-sass": "node-sass --output-style compressed -o web/css --include-path node_modules src/sass/main.scss",
    "optimize-css": "postcss -r web/css",
    "build-css": "npm run build-sass && npm run optimize-css",
    "webpack-build-assets": "webpack --env production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wmde/FundraisingFrontend.git"
  },
  "keywords": [
    "fundraising"
  ],
  "author": "",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/wmde/FundraisingFrontend/issues"
  },
  "homepage": "https://github.com/wmde/FundraisingFrontend#readme",
  "devDependencies": {
    "@csstools/postcss-sass": "^3.0.0",
    "autoprefixer": "^9.1.5",
    "babel-core": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-runtime": "^6.0.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.24.7",
    "browserify": "^13.0.0",
    "browserify-hmr": "^0.3.6",
    "browserify-shim": "^3.8.12",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "deep-freeze": "0.0.1",
    "envify": "^3.4.0",
    "eslint": "^3.3.0",
    "eslint-config-standard": "^5.3.5",
    "eslint-plugin-html": "^1.5.2",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "file-loader": "^2.0.0",
    "glob": "^7.1.3",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.1.0",
    "gulp-changed": "^3.2.0",
    "gulp-clean-css": "^3.10.0",
    "gulp-cli": "^1.4.0",
    "gulp-concat": "^2.6.1",
    "gulp-environments": "^0.1.2",
    "gulp-file-include": "^1.1.0",
    "gulp-flatten": "^0.3.1",
    "gulp-imagemin": "^3.3.0",
    "gulp-inject": "^4.3.2",
    "gulp-jshint": "^2.1.0",
    "gulp-nunjucks-render": "^2.2.2",
    "gulp-sass": "^3.2.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-sync": "^0.1.4",
    "gulp-uglify": "^3.0.1",
    "gulplog": "^1.0.0",
    "imagemin-webpack-plugin": "^2.3.0",
    "jscs": "^2.10.1",
    "jshint": "^2.9.6",
    "mini-css-extract-plugin": "^0.4.3",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-clean": "^1.1.0",
    "postcss-cli": "^6.0.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "sinon": "^1.17.3",
    "style-loader": "^0.23.0",
    "tap-min": "^1.1.0",
    "tape": "^4.9.1",
    "tape-catch": "^1.0.6",
    "uglify-js": "^2.5.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "vueify": "^9.0.0",
    "watchify": "^3.11.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-concat-plugin": "^3.0.0",
    "webpack-config-utils": "^2.3.1"
