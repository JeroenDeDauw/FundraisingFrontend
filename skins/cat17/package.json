    "object-assign": "^4.0.1",
    "polyfill": "^0.1.0",
    "promise": "^7.3.1",
    "redux": "^3.7.2",
    "redux-promise": "^0.5.3",
    "underscore": "^1.9.1",
    "underscore.path": "^0.1.4",
    "vue": "^2.5.17"
  },
  "browserify": {
    "browser": {
      "vue": "vue/dist/vue.common.js"
    },
    "transform": [
      "vueify",
      "babelify",
      "browserify-shim",
      "envify"
    ]
  },
  "browserify-shim": {
    "jquery": "global:jQuery"
  }
}
